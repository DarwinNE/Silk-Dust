typedef unsigned char byte;

void set80col(void);

struct bmp_info {
    unsigned long offset;  
    unsigned int width;    
    unsigned int height;   
};













unsigned short *screen = (unsigned short *)0x3000000;   

void m20_putc(char c)
{
    char bmp_hdr[2];
    if(c=='\n'||c=='\r') {
        _pcos_crlf();
    } else {
        bmp_hdr[0]=c;
        bmp_hdr[1]='\0';
        _pcos_dstring( bmp_hdr ) ;
    }
}

char get_byte(FILE* fin)
{
    static byte c;
    static int l_count;

    if(l_count==0) {
        c= (--(   fin   )->_r < 0 ? __srget(   fin   ) : (int)(*(   fin   )->_p++))   ;
        if(c== 0xD6 ) {
            l_count= (--(   fin   )->_r < 0 ? __srget(   fin   ) : (int)(*(   fin   )->_p++))   -1;
            return c = (--(   fin   )->_r < 0 ? __srget(   fin   ) : (int)(*(   fin   )->_p++))   ;
        }
        return c;
    } else {
        --l_count;
        return c;
    }
}

void wait_key(void)
{
    unsigned char key;

     
    _pcos_getbyte(17 , &key);
}

long int get_32bit(FILE *fin)
{
    return (--(   fin   )->_r < 0 ? __srget(   fin   ) : (int)(*(   fin   )->_p++))    | ((--(   fin   )->_r < 0 ? __srget(   fin   ) : (int)(*(   fin   )->_p++))    << 8) | 
        ((unsigned long)(--(   fin   )->_r < 0 ? __srget(   fin   ) : (int)(*(   fin   )->_p++))    << 16) | ((unsigned long)(--(   fin   )->_r < 0 ? __srget(   fin   ) : (int)(*(   fin   )->_p++))    << 24);
}

void showsplash(void)
{
    unsigned int ix, iy, ep1, ep2, xadj;
    unsigned short *screen_buf;
    struct bmp_info file_info;   

    FILE *fin;

     
    screen_buf = malloc(16384 );
    if (! screen_buf) {
        return;
    }
    memset(screen_buf, 0, 16384 );

     
    fin = fopen("splash.cpr" , "rb");
    if (! fin) {
        free(screen_buf);
        return;
    }

    file_info.width = get_32bit(fin);
    file_info.height = get_32bit(fin);
    

     
    iy = (255 - (256 - file_info.height)) * 32;
    xadj = (256 - file_info.width / 2) / 16;
    while (--file_info.height) {
        for (ix = 0; ix < (file_info.width+16) / 8; ix+=2) {
            ep1 = get_byte(fin);
            ep2 = get_byte(fin);
            *(screen_buf + iy + xadj + (ix >> 1)) = (ep2 | (ep1 << 8));
        }
        iy -= 32;
    }

    memcpy(screen, screen_buf, 16384 );   

    fclose(fin);
    free(screen_buf);

    wait_key();

    set80col();
    _pcos_cls();   

    return;
}
